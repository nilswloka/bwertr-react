{
  "name": "bwertr-react",
  "version": "0.0.1",
  "description": "A JavaScript version of bwertr build with react according to the flux architecture.",
  "main": "src/app.jsx",
  "dependencies": {
    "flux": "2.0.3",
    "react": "0.14.0-beta3"
  },
  "devDependencies": {
    "babel": "*",
    "babel-core": "*",
    "babel-loader": "*",
    "webpack-dev-server": "1.10.1",
    "react-addons-test-utils": "0.14.0-beta3",
    "react-dom": "0.14.0-beta3",
    "karma": "*",
    "karma-cli": "*",
    "karma-jasmine": "*",
    "karma-phantomjs-launcher": "*",
    "karma-webpack": "*",
    "jasmine": "*",
    "core-js": "*",
    "webpack": "1.11.0",
    "style-loader": "0.12.3",
    "css-loader": "0.15.6"
  },
  "scripts": {
    "build": "webpack --progress --profile --colors",
    "dev-server": "webpack-dev-server --progress --colors",
    "test": "karma start"
  },
  "author": "Nils Wloka",
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "testFileExtensions": ["es6", "js"],
    "moduleFileExtensions": ["js", "json", "es6", "jsx"],
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-addons-test-utils",
      "<rootDir>/node_modules/react-dom"
    ]
  }
}
